  Note	       = {technical report},
  Title	       = {Using Restructuring Transformations to Reengineer
                  Object-Oriented Systems},
  Url	       = {http://www.iam.unibe.ch/~demeyer/Deme97z/index.html
                  http://scg.unibe.ch/archive/papers/Deme97zWCRE.pdf},
