  Citeulike-Linkout-1=
                  {http://ieeexplore.ieee.org/xpls/abs\_all.jsp?arnumber=5070019},
  Doi	       = {10.1109/SUITE.2009.5070019},
  Journal      = {Search-Driven Development-Users, Infrastructure,
                  Tools and Evaluation, 2009. SUITE '09. ICSE Workshop
                  on},
  Keywords     = {codesearch, suite2009},
  Pages	       = {37--40},
  Posted-At    = {2009-08-10 11:12:03},
  Priority     = {0},
  Title	       = {Hybrid storage for enabling fully-featured text
                  search and fine-grained structural search over
                  source code},
