  Keywords     = {olit-reuse osg-ftp om90 onhindex(102)},
  Month	       = sep,
  Number       = {9},
  Pages	       = {90--103},
  Title	       = {Class Management for Software Communities},
  Url	       =
                  {http://scg.unibe.ch/archive/osg/Gibb90aClassManagement.pdf},
  Volume       = {33},
  Year	       = {1990},
