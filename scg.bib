  Citeulike-Linkout-1= {http://dx.doi.org/10.1109/MSR.2009.5069501},
  Doi	       = {10.1109/MSR.2009.5069501},
  Isbn	       = {978-1-4244-3493-0},
  Journal      = {Mining Software Repositories, International Workshop
