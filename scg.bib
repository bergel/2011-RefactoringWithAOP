  Number       = {1},
  Pages	       = {26--36},
  Publisher    = {ACM},
  Title	       = {Hidden in Plain Sight},
  Url	       = {http://dx.doi.org/10.1145/1117389.1117401},
  Volume       = {4},
  Year	       = {2006},
  Bdsk-Url-1   = {http://dx.doi.org/10.1145/1117389.1117401}
}

