  volume       = {In Press, Corrected Proof},
  number       = {52},
  pages	       = {1167-1187},
  issn	       = {0950-5849},
  Doi	       = {10.1016/j.infsof.2010.05.010},
  Year	       = {2010},
  Url	       =
                  {http://scg.unibe.ch/archive/papers/Arev10aClassHierarchies.pdf}
}

